#: myvenv/lib/python3.5/site-packages/django/contrib/postgres/apps.py:13
msgid "PostgreSQL extensions"
msgstr "Extensiones de PostgreSQL"

#: myvenv/lib/python3.5/site-packages/django/contrib/postgres/fields/array.py:21
#: myvenv/lib/python3.5/site-packages/django/contrib/postgres/forms/array.py:17
#: myvenv/lib/python3.5/site-packages/django/contrib/postgres/forms/array.py:157
#, python-format
msgid "Item %(nth)s in the array did not validate: "
msgstr "El elemento %(nth)s del arreglo no se pudo validar:"

#: myvenv/lib/python3.5/site-packages/django/contrib/postgres/fields/array.py:22
msgid "Nested arrays must have the same length."
msgstr "Los arreglos anidados deben tener la misma longitud."

#: myvenv/lib/python3.5/site-packages/django/contrib/postgres/fields/hstore.py:16
msgid "Map of strings to strings/nulls"
msgstr "Mapa de cadenas a cadenas/nulos"

#: myvenv/lib/python3.5/site-packages/django/contrib/postgres/fields/hstore.py:18
#, python-format
msgid "The value of \"%(key)s\" is not a string or null."
msgstr "El valor de \"%(key)s\" no es ni una cadena ni un nulo"

#: myvenv/lib/python3.5/site-packages/django/contrib/postgres/fields/jsonb.py:30
msgid "A JSON object"
msgstr "Un objeto JSON"

#: myvenv/lib/python3.5/site-packages/django/contrib/postgres/fields/jsonb.py:32
msgid "Value must be valid JSON."
msgstr "El valor debe ser un JSON válido."

#: myvenv/lib/python3.5/site-packages/django/contrib/postgres/forms/hstore.py:17
msgid "Could not load JSON data."
msgstr "No se pududieron cargar los datos JSON."

#: myvenv/lib/python3.5/site-packages/django/contrib/postgres/forms/hstore.py:18
msgid "Input must be a JSON dictionary."
msgstr "La entrada debe ser un diccionario JSON"

#: myvenv/lib/python3.5/site-packages/django/contrib/postgres/forms/jsonb.py:20
#, python-format
msgid "'%(value)s' value must be valid JSON."
msgstr "El valor \"%(value)s\" debe ser un JSON válido."

#: myvenv/lib/python3.5/site-packages/django/contrib/postgres/forms/ranges.py:13
msgid "Enter two valid values."
msgstr "Introduzca dos valores válidos."

#: myvenv/lib/python3.5/site-packages/django/contrib/postgres/forms/ranges.py:14
msgid "The start of the range must not exceed the end of the range."
msgstr "El comienzo del rango no puede exceder su final."

#: myvenv/lib/python3.5/site-packages/django/contrib/postgres/forms/ranges.py:61
msgid "Enter two whole numbers."
msgstr "Ingrese dos números enteros."

#: myvenv/lib/python3.5/site-packages/django/contrib/postgres/forms/ranges.py:67
msgid "Enter two numbers."
msgstr "Ingrese dos números."

#: myvenv/lib/python3.5/site-packages/django/contrib/postgres/forms/ranges.py:73
msgid "Enter two valid date/times."
msgstr "Ingrese dos fechas/horas válidas."

#: myvenv/lib/python3.5/site-packages/django/contrib/postgres/forms/ranges.py:79
msgid "Enter two valid dates."
msgstr "Ingrese dos fechas válidas."

#: myvenv/lib/python3.5/site-packages/django/contrib/postgres/validators.py:14
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than "
"%(limit_value)d."
msgstr[0] ""
"La lista contiene %(show_value)d elemento, no debería contener más de "
"%(limit_value)d."
msgstr[1] ""
"La lista contiene %(show_value)d elementos, no debería contener más de "
"%(limit_value)d."

#: myvenv/lib/python3.5/site-packages/django/contrib/postgres/validators.py:21
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than "
"%(limit_value)d."
msgstr[0] ""
"La lista contiene %(show_value)d elemento, no debería contener menos de "
"%(limit_value)d."
msgstr[1] ""
"La lista contiene %(show_value)d elementos, no debería contener menos de "
"%(limit_value)d."

#: myvenv/lib/python3.5/site-packages/django/contrib/postgres/validators.py:31
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "Faltan algunas claves: %(keys)s"

#: myvenv/lib/python3.5/site-packages/django/contrib/postgres/validators.py:32
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "Se facilitaron algunas claves desconocidas: %(keys)s"

#: myvenv/lib/python3.5/site-packages/django/contrib/postgres/validators.py:76
#, python-format
msgid ""
"Ensure that this range is completely less than or equal to %(limit_value)s."
msgstr "Asegúrese de que este rango es menor o igual que %(limit_value)s."

#: myvenv/lib/python3.5/site-packages/django/contrib/postgres/validators.py:82
#, python-format
msgid ""
"Ensure that this range is completely greater than or equal to "
"%(limit_value)s."
msgstr ""
"Asegúrese de que este rango es efectivamente mayor o igual que "
"%(limit_value)s."
